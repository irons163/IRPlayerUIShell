ARCHS[sdk=iphonesimulator*] = $(ARCHS_STANDARD_64_BIT)
CLANG_WARN_QUOTED_INCLUDE_IN_FRAMEWORK_HEADER = NO
CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/IRPlayer
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/IRFFMpeg"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1 IRPLATFORM_TARGET_OS_IPHONE_OR_TV IRPLATFORM_TARGET_OS_MAC_OR_IPHONE
HEADER_SEARCH_PATHS = $(inherited) "$(PODS_ROOT)/IRFFMpeg/include" "$(PODS_ROOT)/IRPlayer/IRFFMpeg"
OTHER_LDFLAGS = $(inherited) ${inherited} -l"bz2" -l"iconv" -l"lzma" -l"z"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/IRPlayer
PODS_XCFRAMEWORKS_BUILD_DIR = $(PODS_CONFIGURATION_BUILD_DIR)/XCFrameworkIntermediates
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
